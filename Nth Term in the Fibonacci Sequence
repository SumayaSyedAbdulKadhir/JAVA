import java.util.*;
class Main {
    public static void main(String[] args) {
      

        int input=4;
        int a=1;
        int b=2;
        int c=0;
        for(int i=0;i<input-2;i++){
            c=a+b;
            a=b;
            b=c;
        }
        System.out.println(c);
    }
}
